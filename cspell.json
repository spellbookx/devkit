    "**/.turbo/**",
    "**/.pnpm/**",
    "**/.npm/**",
    "**/.yarn/**",
    "**/.pnp/**",
    "**/.parcel-cache/**",
    "**/.vite/**",
    "**/.rollup.cache/**",
    "**/package-lock.json",
    "**/pnpm-lock.yaml",
    "**/yarn.lock",
    "**/bun.lockb",
    "**/target/**",
    "**/.cargo/**",
    "**/Cargo.lock",
    "**/go.sum",
    "**/go.work",
    "**/go.work.sum",
    "**/vendor/**",
    "**/.gopath/**",
    "**/.cache/go-build/**",
