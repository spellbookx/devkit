  "caseSensitive": false,
  "words": [
    "criscito",
    "davide",
    "dcdavidev",
    "dotenvx",
    "esbenp",
    "famiglio",
    "gopath",
    "hiberfil",
    "ipynb",
    "mjsx",
    "mtsx",
    "stefanzweifel",
    "spellbookx",
    "tabler",
    "vuoto"
  ],
  "dictionaries": [
    "en-gb",
    "en_US",
    "companies",
    "softwareTerms",
    "misc",
    "typescript",
    "node",
    "php",
    "go",
    "python",
    "powershell",
    "html",
    "css",
    "csharp",
    "latex",
    "bash",
    "fonts",
    "fileTypes",
    "npm"
