    "**/env/**",
    "**/.ipynb_checkpoints/**",
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/.idea/**",
    "**/.vscode/**",
    "**/.editorconfig",
    "**/.eslintcache",
    "**/.babelrc",
    "**/.prettier*",
    "**/.nx/**",
    "**/.cspell/**",
    "**/.cursor/**",
    "**/.history/**",
    "**/.sass-cache/**",
    "**/.gradle/**",
    "**/.terraform/**",
    "**/.docker/**",
    "**/.kube/**",
    "**/.devcontainer/**",
    "**/.direnv/**",
